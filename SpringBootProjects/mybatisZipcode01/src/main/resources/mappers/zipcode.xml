<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mybatis.mapper.ZipcodeMapper">
    <select id="listZipcode" parameterType="String" resultType="to">
        select select zipcode, sido, gugun, dong, ri, bunji, seq
        from zipcode
        where dong like #{dong}
    </select>

</mapper>